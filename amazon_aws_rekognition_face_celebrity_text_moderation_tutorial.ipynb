   "source": [
    "%%time\n",
    "\n",
    "getModLabelId = rekognition.get_content_moderation(JobId=modVideoJobId, SortBy=\"TIMESTAMP\")\n",
    "\n",
    "while getModLabelId[\"JobStatus\"] == \"IN_PROGRESS\":\n",
    "    time.sleep(5)\n",
    "    print(\"*\", end=\"\")\n",
    "    getModLabelId = rekognition.get_content_moderation(JobId=modVideoJobId, SortBy=\"TIMESTAMP\")\n",
    "print(\"Detecting Moderation Labels in Video: \"+ getModLabelId[\"JobStatus\"])"
